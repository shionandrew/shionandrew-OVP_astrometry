	title        = {{A Bright Millisecond Radio Burst of Extragalactic Origin}},
	author       = {{Lorimer}, D.~R. and {Bailes}, M. and {McLaughlin}, M.~A. and {Narkevic}, D.~J. and {Crawford}, F.},
	year         = 2007,
	month        = nov,
	journal      = {Science},
