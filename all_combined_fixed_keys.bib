	pages        = 777,
	doi          = {10.1126/science.1147532},
	issn         = {0036-8075},
	keywords     = {ASTRONOMY, Astrophysics},
	archiveprefix = {arXiv},
	eprint       = {0709.4301},
	primaryclass = {astro-ph},
	adsurl       = {https://ui.adsabs.harvard.edu/abs/2007Sci...318..777L},
	adsnote      = {Provided by the SAO/NASA Astrophysics Data System},
	abstractnote = {Pulsar surveys offer a rare opportunity to monitor the radio sky for impulsive burst-like events with millisecond durations. We analyzed archival survey data and found a 30-jansky dispersed burst, less than 5 milliseconds in duration, located 3\textdegree{} from the Small Magellanic Cloud. The burst properties argue against a physical association with our Galaxy or the Small Magellanic Cloud. Current models for the free electron content in the universe imply that the burst is less than 1 gigaparsec distant. No further bursts were seen in 90 hours of additional observations, which implies that it was a singular event such as a supernova or coalescence of relativistic objects. Hundreds of similar events could occur every day and, if detected, could serve as cosmological probes.},
	language     = {English}
}
